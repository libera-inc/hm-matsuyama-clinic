{
    // Live Sass Compilerの設定を指定
    "liveSassCompile.settings.formats": [
        {
            // CSSの出力形式を指定（ここでは「expanded」で展開された形式）
            "format": "expanded",
            // 出力されるファイルの拡張子を指定
            "extensionName": ".css",
            // コンパイルされたCSSファイルの保存先を指定
            "savePath": "/css"
        }
    ],
    "liveSassCompile.settings.showOutputWindowOn": "Warning",
    "liveServer.settings.port": 5501
}
